
{
  "compilerOptions": {
    "target": "ES2020",
	"module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
	"resolveJsonModule": true,
    "outDir": "./dist",
    "rootDir": "src",
	"noImplicitAny": false,
    "skipLibCheck": true,
	"resolveJsonModule": true,  
    "strict": true,
    "baseUrl": "./src",
	"forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx" 
	
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}



    



