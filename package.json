{
  "name": "afcftapp",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build:frontend": "webpack --config webpack.config.js",
    "build:backend": "tsc",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "npm run build && node dist/server.js",
    "dev": "concurrently \"nodemon --watch src --exec ts-node src/server.ts\" \"webpack serve\"",
    "server": "nodemon --watch src --exec ts-node src/server.ts",
    "client": "webpack serve --open"
  },
  "homepage": "https://coderscongress.github.io/afcftapp",
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "node-fetch": "^3.3.2",
    "pdf-lib": "^1.17.1",
    "react-router-dom": "^7.6.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.31",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "axios": "^1.9.0",
    "compression-webpack-plugin": "^11.1.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv": "^16.5.0",
    "html-webpack-plugin": "^5.6.3",
    "i18next": "^25.2.1",
    "nodemon": "^3.1.10",
    "openai": "^5.3.0",
    "react-i18next": "^15.5.2",
    "readline-sync": "^1.4.10",
    "rimraf": "^3.0.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "type": "commonjs"
}
